<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>controller.Login</servlet-class>
        <!--<load-on-startup>1</load-on-startup>-->
    </servlet>
    <servlet>
        <servlet-name>ListarUsuario</servlet-name>
        <servlet-class>controller.ListarUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeletarUsuario</servlet-name>
        <servlet-class>controller.DeletarUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditUsuario</servlet-name>
        <servlet-class>controller.EditUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CommitEditUser</servlet-name>
        <servlet-class>controller.CommitEditUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListarUsuario</servlet-name>
        <url-pattern>/ListarUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeletarUsuario</servlet-name>
        <url-pattern>/DeletarUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditUsuario</servlet-name>
        <url-pattern>/EditUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CommitEditUser</servlet-name>
        <url-pattern>/CommitEditUser</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
